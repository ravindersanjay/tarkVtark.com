# =====================================================================
# Frontend Environment Variables - EXAMPLE FILE
# =====================================================================
# Copy this file to .env and configure for your environment
# This file can be committed to show the structure

# API Base URL - Backend API endpoint
VITE_API_URL=http://localhost:8080/api/v1

# Application Configuration
VITE_APP_NAME=TarkVtark Debate Arena
VITE_APP_VERSION=1.0.0

# Feature Flags (optional)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_LOGS=false

# NOTES:
# - Vite exposes variables as import.meta.env.VITE_*
# - Only VITE_ prefixed variables are exposed to the client
# - Don't put sensitive data here (it's visible in the browser)

